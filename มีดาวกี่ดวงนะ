#include <stdio.h>

int main()
{
	int n;
	scanf_s("%d", &n);
	if(n==1)
	{
		printf("*");
		printf("\nThere have a star.");
	}
	else if(n!=0)
	{
			int i, j;
	for (i = 0; i < n; i++)
	{
			for (j = 0;  j < n ; ++j)
			{
			printf("*");
			}
			printf("\n");
	}
	printf("There have %d stars.",n*n);
	}
	else
	{
		printf("ERROR");
	}
	return 0;
}

